<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Zadoil</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=League+Spartan:wght@100..900&display=swap" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css" />
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
    <link rel="stylesheet" href="./dist/css/main.min.css">
    <link rel="icon" href="/favicon.ico" type="image/x-icon">
    <script src="https://kit.fontawesome.com/7979a03bd9.js" crossorigin="anonymous"></script>
</head>

<body data-bs-spy="scroll" data-bs-target="#navId">

    <!-- @include 'nav.kit' -->

    <section id="rental-offers" class="rental-offers">
        <div class="container">
            <h2 class="section-title">OFERTA WYPOŻYCZALNI</h2>
            <div class="underline"></div>

            <div class="rental-offers__grid">
                <div class="rental-offer" data-aos="fade-up">
                    <div class="rental-offer__img">
                        <img src="./dist/img/machine_01.jpg" alt="Koparka XYZ" class="rental-offer__image">
                    </div>
                    <h3 class="rental-offer__title">Koparka XYZ</h3>
                    <p class="rental-offer__description">
                        Wydajna koparka do prac ziemnych, idealna do wykopów i niwelacji terenu.
                    </p>
                    <a href="#" class="btn btn-outline-light mt-2 text-uppercase">Więcej</a>
                </div>

                <div class="rental-offer" data-aos="fade-up" data-aos-delay="100">
                    <div class="rental-offer__img">
                        <img src="./dist/img/machine_02.jpg" alt="Koparko-ładowarka ABC" class="rental-offer__image">
                    </div>
                    <h3 class="rental-offer__title">Koparko-ładowarka ABC</h3>
                    <p class="rental-offer__description">
                        Wielofunkcyjna maszyna do różnorodnych prac budowlanych i ziemnych.
                    </p>
                    <a href="#" class="btn btn-outline-light mt-2 text-uppercase">Więcej</a>
                </div>

                <div class="rental-offer" data-aos="fade-up" data-aos-delay="200">
                    <div class="rental-offer__img">
                        <img src="./dist/img/machine_03.jpg" alt="Walec DEF" class="rental-offer__image">
                    </div>
                    <h3 class="rental-offer__title">Walec DEF</h3>
                    <p class="rental-offer__description">
                        Niezawodny walec do zagęszczania gruntu i nawierzchni drogowych.
                    </p>
                    <a href="#" class="btn btn-outline-light mt-2 text-uppercase">Więcej</a>
                </div>

            </div>
        </div>
    </section>

    <!-- @include 'footer.kit' -->

    <script type="text/javascript" src="//code.jquery.com/jquery-1.11.0.min.js"></script>
    <script type="text/javascript" src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
    <script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    <script src="./dist/js/aos.min.js"></script>
    <script src="./dist/js/slick.min.js"></script>
    <script src="./dist/js/script.min.js"></script>

</body>

</html>